package app.entity;

public class Page<T> {
    private int page;
    private int size;
    private int totalPage;
    private T data;

    @Override
    public String toString() {
        return "Page{" +
                "page=" +page+
                ",size=" +size+
                ",totalPage=" +totalPage+
                "data=" +data+
                "}";
    }

    public int getPage() {
        return page;
    }

    public int getSize() {
        return size;
    }

    public int getTotalPage() {
        return totalPage;
    }

    public T getData() {
        return data;
    }

    public void setPage(int page) {
        this.page = page;
    }

    public void setData(T data) {
        this.data = data;
    }

    public void setSize(int size) {
        this.size = size;
    }

    public void setTotalPage(int totalPage) {
        this.totalPage = totalPage;
    }
}
