package app.entity.personcenter;

import app.entity.content.BaseContent;

public class MyCollectContent {
    private String publisherName;
    private BaseContent base;

    public BaseContent getBase() {
        return base;
    }

    public void setBase(BaseContent base) {
        this.base = base;
    }

    public String getPublisherName() {
        return publisherName;
    }

    public void setPublisherName(String publisherName) {
        this.publisherName = publisherName;
    }

    @Override
    public String toString() {
        return "MyCollectContent[" +
                "base=" +base+
                ",publisherName='" +publisherName+'\''+
                "]";
    }
}
