html, body {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
}
h1,h2,h3,h4,h5,h6 {
  margin: 0;
}
body {
  color: #FFF;
  background: #48516F;
}
select,input {
  margin: 0;
  padding: 0;
  border: none;
  text-indent: 6px;
}
button {
  border: none;
}
ul {
  margin: 0;
  padding: 0;
}
li {
  list-style: none;
}
p {
  margin: 8px 0;
}
.course_hide{
  display: none;
}
.changePassword{
  cursor: pointer;
  color: rgba(19, 181, 177, 1);
  text-align: center;
  line-height: 30px;
  width: 100px;
  background: #E1EBF0;
  position: absolute;
  height: 30px;
  display: none;
}
.user:hover .changePassword{
  display: block;
}
#noData {
  color: #13b5b1;
  font-size: 20px;
  text-align: center;
  margin-top: 100px;
}
.star:before {
  margin: 5px;
  color: red;
  content: "*";
}

