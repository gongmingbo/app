package app.entity.coursetime;

import app.entity.smart.CourseBaseInfo;

public class CourseDetail {
    private CourseTemplate courseTemplate;
    private CourseBaseInfo courseBaseInfo;

    public CourseBaseInfo getCourseBaseInfo() {
        return courseBaseInfo;
    }

    public void setCourseBaseInfo(CourseBaseInfo courseBaseInfo) {
        this.courseBaseInfo = courseBaseInfo;
    }

    public CourseTemplate getCourseTemplate() {
        return courseTemplate;
    }

    public void setCourseTemplate(CourseTemplate courseTemplate) {
        this.courseTemplate = courseTemplate;
    }

    @Override
    public String toString() {
        return "CourseDetail[" +
                "courseTemplate="+courseTemplate+
                ",courseBaseInfo="+courseBaseInfo+
                "]";
    }
}
